    <section id="fmtgp-numbers" answers_as_choices="true">
        <title>Numbers</title>

        <question id="fmtgp-numbers-proper-divisor">
            <text>Proper Divisor</text>
            <link>https://en.wikipedia.org/wiki/Divisor</link>
            <answer>A divisor of n other than n itself.</answer>
        </question>

        <question id="fmtgp-numbers-aliquot-sum">
            <text>Aliquot Sum</text>
            <link>https://en.wikipedia.org/wiki/Aliquot_sum</link>
            <answer>The sum of all proper divisors of n.</answer>
        </question>

        <question id="fmtgp-numbers-perfect">
            <text>Perfect Number</text>
            <link>https://en.wikipedia.org/wiki/Perfect_number</link>
            <answer>A number that is the sum of its proper divisors.</answer>
        </question>

        <question id="fmtgp-triangular-number">
            <text>Triangular Number</text>
            <link>https://en.wikipedia.org/wiki/Triangular_number</link>
            <answer>Sum of the first n positive integers.</answer>
        </question>

        <question id="fmtgp-numbers-prime-number">
            <text>Prime Number</text>
            <link>https://en.wikipedia.org/wiki/Prime_number</link>
            <answer>A number greater than 1 that has no positive divisors other than 1 and itself.</answer>
        </question>

        <question id="fmtgp-numbers-composite-number">
            <text>Composite Number</text>
            <link>https://en.wikipedia.org/wiki/Composite_number</link>
            <answer>A number that has positive divisors other than 1 and itself.</answer>
        </question>

        <question id="fmtgp-numbers-mersenne-prime">
            <text>Mersenne Prime</text>
            <link>https://en.wikipedia.org/wiki/Mersenne_prime</link>
            <answer>A prime number that is one less than a power of two.</answer>
        </question>

        <question id="fmtgp-numbers-fermat-prime">
            <text>Fermat Prime</text>
            <link>https://en.wikipedia.org/wiki/Fermat_number#Generalized_Fermat_primes</link>
            <answer>A prime number that is one more than a power of two.</answer>
            <note>The power will itself be a power of 2.</note>
        </question>

        <question id="fmtgp-numbers-coprime">
            <text>Coprime Number</text>
            <link>https://en.wikipedia.org/wiki/Coprime_integers</link>
            <answer>Two numbers that have no common divisors greater than 1.</answer>
        </question>

        <question id="fmtgp-numbers-totient">
            <text>Totient</text>
            <link>https://en.wikipedia.org/wiki/Euler's_totient_function</link>
            <answer>The number of positive integers less than n that are coprime with n.</answer>
        </question>

        <question id="fmtgp-numbers-carmichael">
            <text>Carmichael Numbers</text>
            <link>https://en.wikipedia.org/wiki/Carmichael_number</link>
            <answer>Composite number n, greater than 1, whose coprimes up to n satisfy b^(n-1) == 1 mod n.</answer>
        </question>
    </section>

