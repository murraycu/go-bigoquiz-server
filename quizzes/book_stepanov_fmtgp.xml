    <section id="fmtgp-fields" answers_as_choices="true" and_reverse="true">
        <title>Fields</title>

        <question id="fmtgp-fields-field">
            <text>Field</text>
            <link>https://en.wikipedia.org/wiki/Field_(mathematics)</link>
            <answer>An integral domain where every nonzero element is invertible.</answer>
            <note>For instance, rational numbers, real numbers, prime remainder fields, complex numbers.</note>
        </question>

        <question id="fmtgp-fields-subfield">
            <text>Subfield</text>
            <answer>A subset of a field that is itself a field.</answer>
        </question>

        <question id="fmtgp-fields-proper-subfield">
            <text>Proper subfield</text>
            <answer>A subfield different from itself.</answer>
        </question>

        <question id="fmtgp-fields-prime-field">
            <text>Prime Field</text>
            <answer>A field that does not have a proper subfield.</answer>
        </question>
    </section>

